{
    "repositories": [
        {
            "type": "vcs",
            "url": "http://github.com/Qafoo/REST-Micro-Framework"
        },
        {
            "type": "vcs",
            "url": "git://github.com/ezsystems/ezpublish.git"
        }
    ],
    "require": {
        "symfony/symfony": "2.1.0-beta3",
        "twig/twig": "1.9.1",
        "zetacomponents/cache": "dev-master",
        "zetacomponents/php-generator": "dev-master",
        "zetacomponents/configuration": "dev-master",
        "zetacomponents/database": "dev-master",
        "zetacomponents/mail": "1.7",
        "Qafoo/RMF": "dev-master",
        "ezsystems/ezpublish-legacy": "dev-ezpublish5-integration"
    },
    "autoload": {
        "psr-0": {
            "eZ": ""
        }
    },
    "minimum-stability": "dev",
    "config": {
        "process-timeout": 3000
    }
}
