{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
        <img width="20" height="24" alt="eZ Publish Info" style="vertical-align: middle;" src="data:image/png;base64," />
        <span class="sf-toolbar-status sf-toolbar-status-green">{{ collector.count }} call{% if collector.count != 1 %}s{% endif %}</span>
    {% endset %}
    {% set text %}
        <div class="sf-toolbar-info-piece">
                <b>SPI calls</b> <span class="sf-toolbar-status sf-toolbar-status-green">{{ collector.count }}</span>
        </div>
        <div class="sf-toolbar-info-piece">
                <b>SPI handlers</b> <span class="sf-toolbar-status sf-toolbar-status-green">{{ collector.handlerscount }}</span>
        </div>
    {% endset %}
    {% include 'WebProfilerBundle:Profiler:toolbar_item.html.twig' with { 'link': profiler_url } %}
{% endblock %}

{% block menu %}
<span class="label">
    <span class="icon"><img width="20" height="24" alt="eZ Publish Info" style="vertical-align: middle;" src="data:image/png;base64," /></span>
    <strong>eZ Publish</strong>
    <span class="count">
        <span title="SPI calls">{{ collector.count }}</span>
    </span>
</span>
{% endblock %}

{% block panel %}
    <h2>eZ Publish Usage Information</h2>
    <table>
        <tr>
            <th>Total Uncached SPI calls:</th>
            <td>{{ collector.count }}</td>
        </tr>
        {% if collector.handlerscount %}
        <tr>
            <th>Uncached SPI handlers(times loaded):</th>
            <td>{{ collector.handlers|join(', ') }}</td>
        </tr>
        {% endif %}
    </table>

    <h3>Uncached SPI calls</h3>
    <table>
        <tr>
            <th>Class</th>
            <th>Method</th>
            <th>Arguments</th>
        </tr>
        {% for call in collector.calls %}
            <tr>
                <td>{{ call.class }}</td>
                <td>{{ call.method }}</td>
                <td>{{ call.arguments }}</td>
            </tr>
        {% endfor %}
    </table>
{% endblock %}