                    <b>Debug</b>
                    <span>{{ collector.info.debug }}</span>
                </div>

                <div class="sf-toolbar-info-piece">
                    <b>Warning</b>
                    <span>{{ collector.info.warning }}</span>
                </div>

                <div class="sf-toolbar-info-piece">
                    <b>Error</b>
                    <span>{{ collector.info.error }}</span>
                </div>

